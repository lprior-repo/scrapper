{
  "name": "@overseer/monorepo",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot --port 3000 packages/webapp/index.html",
    "dev:api": "go run . api",
    "build": "bun build packages/webapp/src/index.tsx --outdir packages/webapp/build",
    "lint": "eslint packages/webapp/src packages/shared/src --ext .ts,.tsx",
    "lint:fix": "eslint packages/webapp/src packages/shared/src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "test:e2e": "playwright test --config=packages/webapp/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=packages/webapp/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config=packages/webapp/playwright.config.ts --debug",
    "test:visual": "playwright test --config=packages/webapp/playwright.config.ts --grep=\"Visual Regression\"",
    "test:integration": "playwright test --config=packages/webapp/playwright.config.ts --grep=\"Integration\""
  },
  "dependencies": {
    "@effect-rx/rx-react": "^0.37.0",
    "@effect/platform": "^0.58.0",
    "@effect/platform-browser": "^0.58.0",
    "@effect/platform-node": "^0.53.0",
    "@effect/schema": "^0.68.0",
    "@kirkdeam/puppeteer-mcp-server": "^1.1.0",
    "@modelcontextprotocol/server-memory": "^2025.4.25",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.7.1",
    "@playwright/mcp": "^0.0.31",
    "@upstash/context7-mcp": "^1.0.14",
    "cytoscape": "^3.29.2",
    "effect": "^3.16.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "latest",
    "@types/cytoscape": "^3.21.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "bun-types": "latest",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}
